{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/", 
            "methods": ["PUT","GET", "POST", "PATCH", "DELETE", "OPTIONS"]
        }
    ]
}